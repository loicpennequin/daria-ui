{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "components.d.ts",
    "auto-imports.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "docs/**/*",
    "docs/**/*.vue"
  ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "lib": ["es2019"],
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
